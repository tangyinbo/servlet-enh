<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  
  <filter>
  	<filter-name>myFilter</filter-name>
  	<filter-class>com.cowboy.filter.MyFilter</filter-class>
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>bobobobbobo</param-value>
  	</init-param>
  </filter>
  <filter>
  	<filter-name>myFilter2</filter-name>
  	<filter-class>com.cowboy.filter.MyFilter2</filter-class>
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>xixixixixixix</param-value>
  	</init-param>
  </filter>
  <filter-mapping>
  	<filter-name>myFilter2</filter-name>
  	<url-pattern>*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>myFilter</filter-name>
  	<url-pattern>*</url-pattern>
  </filter-mapping>
  <listener>
  	<listener-class>com.cowboy.listener.MyServletContextAttributeListern</listener-class>
  </listener>
  <listener>
  	<listener-class>com.cowboy.listener.MyServeltContextListener</listener-class>
  </listener>
  <listener>
  	<listener-class>com.cowboy.listener.MySessionActivationListener</listener-class>
  </listener>
  <listener>
  	<listener-class>com.cowboy.listener.MySessionListener</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
